{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/transaction-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "tsConfigPath": "apps/transaction-api/tsconfig.app.json"
  },
  "projects": {
    "transaction-api": {
      "type": "application",
      "root": "apps/transaction-api",
      "entryFile": "main",
      "sourceRoot": "apps/transaction-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/transaction-api/tsconfig.app.json"
      }
    },
    "anti-fraud-service": {
      "type": "application",
      "root": "apps/anti-fraud-service",
      "entryFile": "main",
      "sourceRoot": "apps/anti-fraud-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/anti-fraud-service/tsconfig.app.json"
      }
    },
    "shared-types": {
      "type": "library",
      "root": "libs/shared-types",
      "entryFile": "index",
      "sourceRoot": "libs/shared-types/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared-types/tsconfig.lib.json"
      }
    }
  }
}
